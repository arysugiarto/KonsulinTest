<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home"
    app:startDestination="@id/home_fragment">

    <fragment
        android:id="@+id/home_fragment"
        android:name="com.arysugiarto.konsulin.ui.home.HomeFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_home" >

        <action
            android:id="@+id/action_home_fragment_to_onboarding_fragment"
            app:destination="@id/onboarding_fragment" />
    </fragment>

    <fragment
        android:id="@+id/onboarding_fragment"
        android:name="com.arysugiarto.konsulin.ui.slide.OnboardingFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_onboarding" >

    </fragment>

</navigation>